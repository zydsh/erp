INSERT INTO SPR_PS
	VALUES ("9a6a1cac-4906-4fba-b8e5-891006c1be35",
	'RequestEmployeeLeave',
	'An employee requests to have a leave of absence of a specific type. ',
	'//Select an employee using the National ID to deliver the event to the statemachine
// to serve the request for leave.
select any employee from instances of Employee where selected.EmployeeID==param.EmployeeID;
select any leaveSpec from instances of LeaveSpecification where selected.Name ==param.LeaveName;

if ( not empty employee ) and ( not empty leaveSpec)
	//generate EM3:requestLeave(Starting:param.Starting, Ending:param.Ending, Name:param.Name, National_ID:param.National_ID) to employee;
	
	create object instance empLeave of Leave;
	empLeave.Starting = param.Starting;
	empLeave.Ending = param.Ending;
	relate empLeave to employee across R7.''to be taken by'';
	relate empLeave to leaveSpec across R15.''is specified by a'';
	
	//Notify employee manager using the message center
	select one employeeMngr related by employee->Department[R21.''working within'']->Employee[R23.''is managed by''];	
	create object instance approveLeaveMsg of ApproveLeave;
	approveLeaveMsg.createMessage(Starting:param.Starting, Ending:param.Ending, EmployeeID:employee.EmployeeID, ManagerID:employeeMngr.EmployeeID );

	
	//UI::Reply( msg:"Employee has requested leave successfully ", state:True);

else 
	LOG::LogInfo(message:"Employee is not registered!" );
	UI::Reply( msg:"Employee is not found.", state:False);
end if;
',
	1,
	0);
