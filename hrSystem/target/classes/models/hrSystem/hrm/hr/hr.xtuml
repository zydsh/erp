	VALUES ("3651b838-5ea1-4197-b710-d25799ce3be8",
	'CommenceEmployee',
	'',
	' //Employee::commenceEmployee( National_ID:param.National_ID ); 
 
 select any employee from instances of Employee where selected.NationalID==param.National_ID;
 if ( not empty employee )
 	employee.commenceEmployee( National_ID:param.National_ID);
	
